            }
          },
          "title": "Geometric",
          "type": "object"
        },
        "_logistic": {
          "description": "Logistic",
          "properties": {
            "location": {
              "default": 0,
              "required": true,
              "type": "number"
            },
            "scale": {
              "default": 0,
              "required": true,
              "type": "number"
            }
          },
          "title": "Logistic",
          "type": "object"
        },
        "_lognormal": {
          "description": "Lognormal",
          "properties": {
            "mean": {
              "_class": "Dream.Property",
              "default": 0,
              "name": "Mean",
              "type": "number"
            },
            "stdev": {
              "_class": "Dream.Property",
              "default": 0,
